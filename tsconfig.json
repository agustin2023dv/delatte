{
  "compilerOptions": {
    "target": "es5", 
    "lib": ["dom", "es2017"],
    "types": ["node"],
    "typeRoots": ["./node_modules/@types"],
    "jsx": "react-native",
    "moduleResolution": "node", 
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./", 
    "paths": {
      "@/app/*": ["app/*"],
      "backend/*": ["backend/src/*"], 
      "shared/*": ["shared/*"] 
    }
  },
  "include": [
  "app/**/*", 
  "backend/**/*", 
  "shared/**/*", 
  "types.d.ts",
  ".expo/types/**/*.ts",
  "expo-env.d.ts"
, "components/navbar"  ],
  "exclude": ["node_modules", ".vscode", "dist", "backend/node_modules"], 
  "extends": "expo/tsconfig.base"
}